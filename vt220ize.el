(defvar *char*)
(defvar *text*)
(defvar *sixel*)

(defun pixel (x y)
  (nth x (nth y *picture*)))

(defun sixel (x y h)
  (let ((data 0))
    (loop for dy from 0 below h do
         (message "%d %d %d %d" x y dy h)
         (if (= (pixel x (+ y dy)) 1)
           (setq data (logior data (lsh 1 dy)))))
    (unless (zerop data)
      (setq *blankp* nil))
    (push (+ data #o77) *sixel*)
    data))

(defun char (x y)
  (let ((*blankp* t)
        (*sixel* nil))
    (loop for dx from 0 below 8 do
         (sixel (+ x dx) y 6))
    (push ?/ *sixel*)
    (loop for dx from 0 below 8 do
         (sixel (+ x dx) (+ y 6) 4))
    (push #o73 *sixel*)
    (if *blankp*
        (push ?  *text*)
      (push *char* *text*)
      (incf *char*)
      (insert (concat "" (nreverse *sixel*))))))

(defun row (y)
  (loop for x from 0 below (length (first *picture*)) by 8
     do (char x y))
  (push ?\n *text*)
  (push ?\r *text*))

(defun pic ()
  (insert "\eP1;1;1{ @")
  (setq *text* nil
        *char* 33)
  (loop for y from 0 below (length *picture*) by 10
     do (row y))
  (insert "\e\\")
  (insert "\e( @")
  (insert (concat "" (nreverse *text*)))
  (insert "\e(B"))

(pic)
